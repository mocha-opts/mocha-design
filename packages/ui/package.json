{
  "name": "@mocha-design/ui",
  "version": "0.0.0",
  "description": "An infrastructure monorepo ui library for all front-end apps.",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "types": "es/react/index.d.ts",
  "sideEffects": [
    "**/*.css"
  ],
  "browser": {
    "./react": "./es/react/index.js"
  },
  "exports": {
    "./react": {
      "import": "./es/react/index.js",
      "require": "./lib/react/index.cjs"
    },
    "./styles.css": "./dist/react/index.css"
  },
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev": "rollup --config rollup.config.js --watch",
    "build": "rollup --config rollup.config.js",
    "prepare": "npm run build",
    "check-types": "tsc --noEmit",
    "build-tsup": "tsup src/index.tsx --format cjs --dts --external react",
    "clean-tsup": "rm -rf dist",
    "dev-tsup": "tsup src/index.tsx --format cjs --watch --dts --external react",
    "lint-tsup": "eslint \"react/**/*.ts*\""
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/node": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.5",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/runtime": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vue/compiler-sfc": "^3.2.27",
    "eslint": "^8.4.1",
    "@mocha-design/eslint-config": "workspace:*",
    "postcss": "^8.4.20",
    "react": "^18.2.0",
    "rollup": "^2.60.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "rollup-plugin-vue": "^6.0.0",
    "@mocha-design/tailwind-config": "workspace:*",
    "@mocha-design/tsconfig": "workspace:*",
    "tslib": "^2.3.0",
    "tsup": "^6.1.3",
    "typescript": "^4.9.4",
    "autoprefixer": "^10.4.14"
  }

}
