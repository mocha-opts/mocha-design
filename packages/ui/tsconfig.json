{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@coconut-killer/ui": ["src"],
      "@coconut-killer/ui/*": ["src/*", "*"]
